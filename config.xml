<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.gcoxsg" version="1.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GCOX-SG</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AppendUserAgent" value="InAppWebView" />
    <preference name="orientation" value="portrait" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/gcox.png" />
        <icon density="mdpi" src="resources/android/icon/gcox.png" />
        <icon density="hdpi" src="resources/android/icon/gcox.png" />
        <icon density="xhdpi" src="resources/android/icon/gcox.png" />
        <icon density="xxhdpi" src="resources/android/icon/gcox.png" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:usesCleartextTraffic="true" />
        </edit-config>
    </platform>
    <chcp>
        <config-file url="http://res.ssss.so/hotfix/gcox-sg/www/chcp.json" />
        <auto-download enabled="false" />
        <auto-install enabled="false" />
        <native-interface version="1" />
    </chcp>
    <engine name="ios" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.3" />
    <plugin name="cordova-plugin-share" spec="^0.1.3" />
    <plugin name="nl.madebymark.share" spec="~0.1.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.4" />
    <plugin name="cordova-hot-code-push-plugin" spec="^1.5.3" />
    <plugin name="phonegap-plugin-barcodescanner" spec="^8.1.0">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-appversion" spec="^1.0.0" />
    <plugin name="cordova-plugin-app-update" spec="git+ssh://git@c.koall.io:ko-plugins/cordova-plugin-app-update.git" />
    <engine name="android" spec="^7.0.0" />
</widget>
